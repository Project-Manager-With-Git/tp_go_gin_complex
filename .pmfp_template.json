{
    "language": "go",
    "env": "gomod",
    "description": "gin构造复杂服务的模板,支持RESTful的api,sse和流式下载大文件,注意使用`swag init`创建docs文件夹",
    "author": "hsz",
    "template_type": "socket",
    "requires": [
        "github.com/Golang-Tools/jwthelper@v0.0.4",
        "github.com/Golang-Tools/loggerhelper@v0.0.4",
        "github.com/Golang-Tools/redishelper@v0.0.7",
        "github.com/Golang-Tools/schema-entry-go@v0.0.7",
        "github.com/Golang-Tools/sqlhelper@v0.0.7",
        "github.com/alecthomas/jsonschema@v0.0.0-20210920000243-787cd8204a0d",
        "github.com/gin-contrib/cors@v1.3.1",
        "github.com/gin-contrib/sse@v0.1.0",
        "github.com/gin-contrib/static@v0.0.1",
        "github.com/gin-gonic/gin@v1.7.4",
        "github.com/json-iterator/go@v1.1.12",
        "github.com/satori/go.uuid@v1.2.0",
        "github.com/scylladb/go-set@v1.0.2",
        "github.com/swaggo/files@v0.0.0-20210815190702-a29dd2bc99b2",
        "github.com/swaggo/gin-swagger@v1.3.1",
        "github.com/swaggo/swag@v1.7.1",
        "github.com/toorop/gin-logrus@v0.0.0-20210225092905-2c785434f26f",
        "github.com/uptrace/bun@v1.0.8",
        "github.com/xeipuuv/gojsonschema@v1.2.0"
    ],
    "template_keys": {
        "app_name": {
            "description": "应用名",
            "default": "{{ project_name }}"
        },
        "api_name": {
            "description": "应用名",
            "default": "ping"
        },
        "apinamespace": {
            "description": "api命名空间名",
            "default": "user"
        },
        "downloadnamespace": {
            "description": "下载命名空间名",
            "default": "table"
        },
        "eventnamespace": {
            "description": "事件命名空间名",
            "default": "timer"
        },
        "author": {
            "description": "作者",
            "default": "{{ author }}"
        },
        "author_email": {
            "description": "作者",
            "default": "{{ author_email }}"
        },
        "version": {
            "description": "项目版本",
            "default": "{{ version }}"
        }
    },
    "test": {
        "source": "cp_go_gin//test",
        "default_path": "servtest"
    },
    "components": {
        "apis_init": {
            "source": "cp_go_gin@v0.0.1//apis_init",
            "description": "RESTfulapi的存放位置",
            "default_path": "apis/apis.go"
        },
        "apis_simple": {
            "source": "cp_go_gin@v0.0.1//apis_simple",
            "description": "RESTfulapi的存放位置",
            "default_path": "apis/{{ api_name }}.go"
        },
        "apis_namespace": {
            "source": "cp_go_gin@v0.0.1//apis_namespace",
            "description": "RESTfulapi的存放位置",
            "default_path": "apis/{{ apinamespace }}namespace"
        },
        "downloads_init": {
            "source": "cp_go_gin@v0.0.1//downloads_init",
            "description": "",
            "default_path": "downloads/downloads.go"
        },
        "downloads_namespace": {
            "source": "cp_go_gin@v0.0.1//downloads_namespace",
            "description": "",
            "default_path": "downloads/{{ downloadnamespace }}namespace"
        },
        "events_init": {
            "source": "cp_go_gin@v0.0.1//events_init",
            "description": "sse接口定义位置",
            "default_path": "events/events.go"
        },
        "events_setting": {
            "source": "cp_go_gin@v0.0.1//events_setting",
            "description": "sse接口定义位置",
            "default_path": "events/setting"
        },
        "events_namespace": {
            "source": "cp_go_gin@v0.0.1//events_namespace",
            "description": "sse接口定义位置",
            "default_path": "events/{{ eventnamespace }}namespace"
        },
        "main": {
            "source": "cp_go_gin@v0.0.1//main",
            "description": "入口函数位置",
            "default_path": "main.go"
        },
        "models": {
            "source": "cp_go_gin@v0.0.1//models",
            "description": "数据模型存放位置,治理使用xormplus作为orm",
            "default_path": "models"
        },
        "serv": {
            "source": "serv",
            "description": "服务器存放位置",
            "default_path": "serv"
        },
        "docker-compose": {
            "source": "cp_dockercompose@v0.0.2//server_standalone",
            "description": "通用的服务使用的dockercompose,适用于调试",
            "default_path": "docker-compose.yml"
        },
        "dockerfile": {
            "source": "cp_dockerfiles@v0.0.1//go_http_pure_dockerfile",
            "description": "构造gin项目的dockerfile",
            "default_path": "dockerfile"
        }
    }
}