{
    "language": "go",
    "env": "gomod",
    "description": "gin构造复杂服务的模板,支持RESTful的api,sse和流式下载大文件,注意使用`swag init`创建docs文件夹",
    "author": "hsz",
    "template_type": "socket",
    "requires": [
        "github.com/Chronokeeper/anyxml@v0.0.0-20160530174208-54457d8e98c6@//@indirect",
        "github.com/CloudyKit/fastprinter@v0.0.0-20200109182630-33d98a066a53@//@indirect",
        "github.com/CloudyKit/jet@v2.1.2+incompatible@//@indirect",
        "github.com/Golang-Tools/loggerhelper@v0.0.3",
        "github.com/Golang-Tools/redishelper@v0.0.3",
        "github.com/Golang-Tools/schema-entry-go@v0.0.4",
        "github.com/agrison/go-tablib@v0.0.0-20160310143025-4930582c22ee@//@indirect",
        "github.com/agrison/mxj@v0.0.0-20160310142625-1269f8afb3b4@//@indirect",
        "github.com/alecthomas/template@v0.0.0-20190718012654-fb15b899a751",
        "github.com/bndr/gotabulate@v1.1.2@//@indirect",
        "github.com/clbanning/mxj@v1.8.4@//@indirect",
        "github.com/fatih/structs@v1.1.0@//@indirect",
        "github.com/gin-contrib/cors@v1.3.1",
        "github.com/gin-contrib/sse@v0.1.0",
        "github.com/gin-contrib/static@v0.0.1",
        "github.com/gin-gonic/gin@v1.7.1",
        "github.com/go-sql-driver/mysql@v1.6.0@//@indirect",
        "github.com/json-iterator/go@v1.1.10",
        "github.com/mattn/go-sqlite3@v1.14.7",
        "github.com/r3labs/sse/v2@v2.3.3",
        "github.com/satori/go.uuid@v1.2.0",
        "github.com/stretchr/testify@v1.7.0",
        "github.com/swaggo/files@v0.0.0-20190704085106-630677cd5c14",
        "github.com/swaggo/gin-swagger@v1.3.0",
        "github.com/swaggo/swag@v1.7.0",
        "github.com/syndtr/goleveldb@v1.0.0@//@indirect",
        "github.com/tealeg/xlsx@v1.0.5@//@indirect",
        "github.com/toorop/gin-logrus@v0.0.0-20210225092905-2c785434f26f",
        "github.com/xormplus/builder@v0.0.0-20200331055651-240ff40009be@//@indirect",
        "github.com/xormplus/xorm@v0.0.0-20210512135344-8123d584d5f5",
        "gopkg.in/flosch/pongo2.v3@v3.0.0-20141028000813-5e81b817a0c4@//@indirect"
    ],
    "template_keys": {
        "app_name": {
            "description": "应用名",
            "default": "{{ project_name }}"
        },
        "api_name": {
            "description": "应用名",
            "default": "ping"
        },
        "apinamespace": {
            "description": "api命名空间名",
            "default": "user"
        },
        "downloadnamespace": {
            "description": "下载命名空间名",
            "default": "table"
        },
        "eventnamespace": {
            "description": "事件命名空间名",
            "default": "timer"
        },
        "author": {
            "description": "作者",
            "default": "{{ author }}"
        },
        "author_email": {
            "description": "作者",
            "default": "{{ author_email }}"
        },
        "version": {
            "description": "项目版本",
            "default": "{{ version }}"
        }
    },
    "test": {
        "source": "cp_go_gin//test",
        "default_path": "servtest"
    },
    "components": {
        "apis_init": {
            "source": "cp_go_gin//apis_init",
            "description": "RESTfulapi的存放位置",
            "default_path": "apis/apis.go"
        },
        "apis_simple": {
            "source": "cp_go_gin//apis_simple",
            "description": "RESTfulapi的存放位置",
            "default_path": "apis/{{ api_name }}.go"
        },
        "apis_namespace": {
            "source": "cp_go_gin//apis_namespace",
            "description": "RESTfulapi的存放位置",
            "default_path": "apis/{{ apinamespace }}namespace"
        },
        "downloads_init": {
            "source": "cp_go_gin//downloads_init",
            "description": "",
            "default_path": "downloads/downloads.go"
        },
        "downloads_namespace": {
            "source": "cp_go_gin//downloads_namespace",
            "description": "",
            "default_path": "downloads/{{ downloadnamespace }}namespace"
        },
        "events_init": {
            "source": "cp_go_gin//events_init",
            "description": "sse接口定义位置",
            "default_path": "events/events.go"
        },
        "events_setting": {
            "source": "cp_go_gin//events_setting",
            "description": "sse接口定义位置",
            "default_path": "events/setting"
        },
        "events_namespace": {
            "source": "cp_go_gin//events_namespace",
            "description": "sse接口定义位置",
            "default_path": "events/{{ eventnamespace }}namespace"
        },
        "main": {
            "source": "cp_go_gin//main",
            "description": "入口函数位置",
            "default_path": "main.go"
        },
        "models": {
            "source": "cp_go_gin//models",
            "description": "数据模型存放位置,治理使用xormplus作为orm",
            "default_path": "models"
        },
        "serv": {
            "source": "serv",
            "description": "服务器存放位置",
            "default_path": "serv"
        },
        "docker-compose": {
            "source": "cp_dockercompose//server_standalone",
            "description": "通用的服务使用的dockercompose,适用于调试",
            "default_path": "docker-compose.yml"
        },
        "dockerfile": {
            "source": "cp_dockerfiles//go_http_pure_dockerfile",
            "description": "构造gin项目的dockerfile",
            "default_path": "dockerfile"
        }
    }
}